{
  "name": "flying-redux-template",
  "displayName": "flying-redux-template",
  "description": "Generate flying-redux Template",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flying-redux-template.helloWorld",
        "title": "Hello World",
        "category": "Flying-Redux Template"
      },
      {
        "command": "flying-redux-template.page",
        "title": "Generate Page Template",
        "category": "Flying-Redux Template"
      },
      {
        "command": "flying-redux-template.component",
        "title": "Generate Component Template",
        "category": "Flying-Redux-Template"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "flying-redux-template.helloWorld",
          "group": "flying-redux-template"
        },
        {
          "command": "flying-redux-template.page",
          "group": "flying-redux-template"
        },
        {
          "command": "flying-redux-template.component",
          "group": "flying-redux-template"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/mz": "^2.7.4",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "i": "^0.3.7",
    "mz": "^2.7.0"
  }
}
